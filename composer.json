{
    "name": "rennokki/chargeswarm",
    "description": "Chargeswarm is a Laravel package that helps you handling subscriptions using the Chargebee service.",
    "keywords": [
        "eloquent",
        "laravel",
        "subscription",
        "plan",
        "plans",
        "model",
        "saas",
        "chargebee",
        "swarm",
        "braintree",
        "stripe",
        "billing"
    ],
    "homepage": "https://github.com/rennokki/chargeswarm",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Renoki",
            "email": "rennokki@gmail.com",
            "homepage": "https://twitter.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "chargebee/chargebee-php": "^2.5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0|^8.0",
        "orchestra/testbench": "~3.5.0|~3.6.0",
        "orchestra/database": "~3.5.0|~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "Rennokki\\Chargeswarm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rennokki\\Chargeswarm\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rennokki\\Chargeswarm\\ChargeswarmServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
